                       android:layout_height="fill_parent"
                       android:drawableLeft="@drawable/shuangdian"
                       android:drawablePadding="10.0dip"
                       android:gravity="center_vertical"
                       android:includeFontPadding="false"
                       android:paddingLeft="15.0dip"
                       android:text="我的爽点"
                       android:textColor="#949494"
                       android:textSize="17sp" />

                   <ImageView
                       android:layout_width="wrap_content"
                       android:layout_height="fill_parent"
                       android:layout_gravity="right"
                       android:gravity="center_vertical"
                       android:paddingRight="20.0dip"
                       android:src="@drawable/ic_arrow" />
              </TableRow> 
        
			<!-- 第二排 -->
            <TableRow
                android:id="@+id/wodeshoucang"
                android:layout_width="fill_parent"
                  android:background="@drawable/title_layout_selector"
                android:layout_marginLeft="2.0dip"
                android:layout_marginRight="2.0dip"
                android:layout_marginTop="15dp"
                android:paddingBottom="16.0dip"
                android:paddingTop="16.0dip"
                android:clickable="true" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:drawableLeft="@drawable/wodeguanzhu"
                    android:drawablePadding="10.0dip"
                    android:gravity="center_vertical"
                    android:includeFontPadding="false"
                    android:paddingLeft="15.0dip"
                    android:text="我的收藏"
                    android:textColor="#949494"
                    android:textSize="17sp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_gravity="right"
                    android:gravity="center_vertical"
                    android:paddingRight="20.0dip"
                    android:src="@drawable/ic_arrow" />
          </TableRow>

          <TableRow
              android:id="@+id/wodexiaoxi"
              android:layout_width="fill_parent"
                android:background="@drawable/title_layout_selector"
              android:layout_marginLeft="2.0dip"
              android:layout_marginRight="2.0dip"
              android:layout_marginTop="1dp"
              android:paddingBottom="16.0dip"
              android:paddingTop="16.0dip"
              android:clickable="true" >

              <TextView
                  android:layout_width="wrap_content"
                  android:layout_height="fill_parent"
                  android:drawableLeft="@drawable/wodexiaoxi"
                  android:drawablePadding="10.0dip"
