<?xml version="1.0" encoding="utf-8"?>

	<LinearLayout
	    xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/linearLayout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#f3f4f6"
        android:orientation="vertical" >

       <!-- 顶部导航 -->
       <RelativeLayout
            android:id="@+id/relativeLayout1"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="#7c7c7c" >

           <!--  <Button
                android:id="@+id/shezhi_button"
                android:layout_width="70dp"
                android:layout_height="match_parent"
                android:layout_alignParentLeft="true"
               android:layout_centerVertical="true"    
                android:text="设置"
                android:paddingLeft="5dp"
              	android:gravity="left|center_vertical"
              	android:background="@drawable/wode_title_btn_selector"
                android:textColor="#fbfdfc"
                android:textSize="15sp" /> -->

            <TextView
                android:id="@+id/text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
	            android:layout_marginBottom="10dp"
	            android:layout_marginTop="10dp"
                android:gravity="center_horizontal"
                android:text="我的地狗"
                android:textColor="#fbfdfc"
                android:textSize="22sp" />

            <Button
                android:id="@+id/moshi_button"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"    
	            android:padding="5dp"
                android:text="商家模式"
                android:textColor="#fbfdfc"
                android:background="@drawable/wode_title_btn_selector"
                android:textSize="15sp" />
        </RelativeLayout>

       
       <com.handmark.pulltorefresh.library.PullToRefreshScrollView
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:id="@+id/wode_fragment_srollview"
           >
           
           <LinearLayout
               
                android:orientation="vertical"
               android:layout_width="match_parent"
               android:layout_height="wrap_content">
               
               
               
               <LinearLayout
            android:id="@+id/relativeLayout2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center_vertical"
            android:background="@drawable/beijing">

            <RelativeLayout 
                android:id="@+id/wode_layout_upload_image"
              	android:clickable="true"
                android:layout_width="120dp"
                android:layout_height="120dp"
                >

                
	            <ImageView
	                android:id="@+id/wode_fragment_imv_image"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
  	              
	                android:background="@drawable/touxiang" />
	            
	            <ImageView
	                android:id="@+id/imageView2"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:layout_alignParentBottom="true"
	                android:layout_alignRight="@+id/wode_fragment_imv_image"
	                android:src="@drawable/xiangji" />
	               
            </RelativeLayout>
                     
            <LinearLayout
                android:id="@+id/linearLayout3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
               	
               	android:layout_marginLeft="30dp"
               	android:layout_gravity="center_vertical"
                >

	                
	            <TextView
	                android:id="@+id/wode_fragment_username"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	               
	                android:text="admin"
	                android:textColor="#949496" />

	            <TextView
	                android:id="@+id/wode_fragment_level"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:text="v1"
	                android:layout_marginTop="10dp"
	                android:layout_marginBottom="10dp"
	                android:textColor="#949496" />

	            <LinearLayout
	                android:id="@+id/linearLayout4"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
                	android:orientation="horizontal"
                	android:layout_alignParentLeft="true"
	                android:layout_toRightOf="@+id/imageView1"
	                android:gravity="left" >
	                
	                	
		            <TextView
		               
		                android:layout_width="wrap_content"
		                android:layout_height="wrap_content"
                		
	                	android:textColor="#949496" 
		                android:text="爽点" />
	                
  			            <TextView
		                android:id="@+id/wode_fragment_shuangdian"
		                android:layout_width="wrap_content"
		                android:layout_height="wrap_content"
		                android:layout_marginLeft="10dp"
		                android:text="10" 
	                	android:textColor="#f44624" />
	            </LinearLayout>              
            </LinearLayout>
        </LinearLayout>
               
               
           <TableLayout
                 android:id="@+id/MorePageTableLayout_Follow"
                 android:layout_width="fill_parent"
                 android:layout_height="wrap_content"
             	 android:layout_marginTop="15dp"
                 android:paddingLeft="1.0dip"
                 android:shrinkColumns="0"
                 android:stretchColumns="0" 
                 >
		<!-- 第一排 -->
                 <TableRow
                     android:id="@+id/dingdanchaxun"
                     android:layout_width="fill_parent"
                    
                     android:layout_marginLeft="2.0dip"
                     android:layout_marginRight="2.0dip"
                     android:paddingBottom="16.0dip"
                     android:paddingTop="16.0dip"
                     android:background="@drawable/title_layout_selector"
                     android:clickable="true">

                     <TextView
                         android:layout_width="wrap_content"
                         android:layout_height="fill_parent"
                         android:drawableLeft="@drawable/dingdanchaxun"
                         android:drawablePadding="10.0dip"
                         android:gravity="center_vertical"
                         android:includeFontPadding="false"
                         android:paddingLeft="15.0dip"
                         android:text="订单查询"
                         android:textColor="#949494"
                         android:textSize="17sp" />

                     <ImageView
                         android:layout_width="wrap_content"
                         android:layout_height="fill_parent"
                         android:layout_gravity="right"
                         android:gravity="center_vertical"
                         android:paddingRight="20.0dip"
                         android:src="@drawable/ic_arrow" />
               </TableRow>

               <TableRow
                   android:id="@+id/shuangdian"
                   android:layout_width="fill_parent"
                   android:background="@drawable/title_layout_selector"
